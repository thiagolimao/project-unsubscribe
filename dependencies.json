{
    "jquery": {
        "src": [
            "./node_modules/jquery/dist/jquery.min.js",
            "./node_modules/jquery/dist/jquery.min.map"
        ],
        "dest": "assets/vendor/jquery/",
        "js_link": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
    },
    "bootstrap": {
        "src": "./node_modules/bootstrap/dist/**/*",
        "dest": "assets/vendor/bootstrap/",
        "css_link": "https://cdnjs.cloudflare.com/ajax/libs/bootstrap/4.6.1/css/bootstrap.min.css",
        "js_link": "https://cdnjs.cloudflare.com/ajax/libs/bootstrap/4.6.1/js/bootstrap.bundle.min.js"
    },
    "font-awesome": {
        "srcs": [
            {
                "src": "./node_modules/font-awesome/css/*",
                "dest": "assets/vendor/font-awesome/css/"
            },
            {
                "src": "./node_modules/font-awesome/fonts/*",
                "dest": "assets/vendor/font-awesome/fonts/"
            }
        ],
        "css_link": "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css",
        "js_link": "https://kit.fontawesome.com/2a10ab39d6.js"
    },
    "aos": {
        "src": "./node_modules/aos/dist/**/*",
        "dest": "assets/vendor/aos/",
        "css_link": "https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css",
        "js_link": "https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"
    }
}